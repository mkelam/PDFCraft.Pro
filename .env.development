# PDF SaaS Platform - Development Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_BASE_URL=ws://localhost:8080

# File Processing Limits
VITE_MAX_FILE_SIZE_MB=100
VITE_SUPPORTED_FORMATS=pdf,docx

# Performance Settings
VITE_GLASSMORPHIC_BLUR_MOBILE=8
VITE_GLASSMORPHIC_BLUR_DESKTOP=20
VITE_ANIMATION_DURATION_MS=200
VITE_DEBOUNCE_MS=300

# Feature Flags
VITE_ENABLE_WEBSOCKET_UPDATES=true
VITE_ENABLE_BATCH_PROCESSING=true
VITE_ENABLE_PERFORMANCE_METRICS=true
VITE_ENABLE_MOCK_BACKEND=true

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug